package StaticControlFlow;



public class ClassAbc {
static int i=10;
static {
	
	 Mussoorie() ; ramjhula();
	
	  methodOne(); methodtwo(); controll(); commerial(); methodConstant();
	  methodrpg ();
	System.out.println("first static block");
	
	
     }


public static void main(String[] args) {
	
	
	  ramjhula(); methodOne(); methodtwo(); controll(); commerial();
	  methodConstant(); methodeuh(); methodabc(); methodrpg(); Mussoorie() ;
	 
	System.out.println("i am main method");
}

 public static void methodOne() {
	 System.out.println(f);
	
 }
 
 static {
	 System.out.println("second static block");
	 
 }
 
 static float f=1.236783453274837f;
	


	public static void methodtwo() {
		System.out.println(h);
	}
	
	static {
		System.out.println("third static block");
		
	}
	
	static double h=32.7634;
	
	
	public static void controll() {
		System.out.println( l);
		
		
	}
	
	static {
		System.out.println("fourth static block");
		
		
	}
	
	static int l=43;


    public static void commerial() {
    	System.out.println(g);
    }

    static {
    	System.out.println("you are so pritty girl ");
    	
    	
    }
    
    static boolean g=true;
    
    
    public static void methodConstant() {
    	System.out.println(m);
    	
    	
    }
    
    static {
    	System.out.println("hiii beautiful");
    	
    }
    
    
    static byte m=123;
    
    public static void methodabc() {
    	System.out.println(o);
    }
    static {
    	System.out.println("hey where are you going in that time");
    }
    
    static char o=('a');
   
    public static void methodeuh() {
    System.out.println(s);	
    
    }
    
    
    static {
    	System.out.println("hii beautiful what are you doing");
    
    }
    
    
    static char s=('v');
    
    public static void methodrpg() {
    	System.out.println(e);
    	
    }
    
    static {
    	System.out.println("you are so lucky ");
    	
    }
    
    static short e=1324;
    
    
    
    public static void Mussoorie() {
    	System.out.println(q);
    	
    	 }
    static {
    	System.out.println("i know that i am not best for you");
    	
    }
    
    static int q=78;
    
    
     public static void ramjhula() {
    	System.out.println(d); 
    	
    	}
     
     static {
    	 System.out.println("ramjhula is best place of rishikesh");
    	 
    	  }
     static int d=425;
     
     
} 
     
     
     
     
     
     
     
     
     
