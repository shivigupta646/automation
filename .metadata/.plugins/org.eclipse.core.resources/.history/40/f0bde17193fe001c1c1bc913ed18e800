package forloop;

public class table {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		
		for(int i=0;i<=100;i++) {
			System.out.println(1+i);
		}
		
		for(int i=0;i<=20;i++) {
			System.out.println("2*"+i+"="+(i+i));
		}
		
		for(int i=0;i<=15;i++) {
			System.out.println(i+"*"+i+"*"+(i*i*i));
		}
		
		for(int i=0;i<=30;i++) {
			System.out.println("2*"+i+"="+(i*i));
		}
		
		
		for(int i=0;i<=20;i++) {
			System.out.println(i+"*"+i+"*"+i+"*"+(i*i*i*i));
		}
		
		
		for(int i=0;i<=40;i++) {
			System.out.println(i+"*"+i+"*"+i+"*"+i+"*"+(i*i*i*i*i));
		}
			
		for(int i=0;i<=48;i++)	{
			System.out.println(1+1*i*i+1+7+9*i);
		}
			
			
			
			
			
			
		
	}

}
