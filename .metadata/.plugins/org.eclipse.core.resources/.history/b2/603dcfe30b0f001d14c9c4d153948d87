package mypackage1;

public class xd {
	public static void main(String[] args) {
		m();m2();
	}
	public static void m() {
for (int i=0;i<=4;i++) {
	for(int j=0; j<=4;j++) {
		System.out.print("* ");
	}
	System.out.print(i);
}
	}
	public static void m2() {
for(int a=1;a<=1;a++) {
	System.out.println(a);
}
}}