package importantloop;

public class examplefor {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		
	for(int i=0; i<=15; i++) {
		System.out.println("2*"+i+"="+(i*i));
	}
		
	for(int i=0; i<=20; i++) {
		System.out.println("2*"+i+"="+(2*i));
	}
	
	

	
	for(int i=0; i<=10; i++) {
		System.out.println("2"+i+"="+(2*i));
	}
	
	
	
	
	for(char i=0; i<=10; i++) {
		System.out.println('a');
	}
	
	for(int i=0; i<=45; i++) {
		System.out.println(i+"*"+i+"="+(i*i));
	}
	
	
	for (int i=0; i<=100; i++) {
		System.out.println(i+i);
	}
	
	
	for(int i=0; i<=100; i++) {
		System.out.println(1+i);
	}
	
	
	for(int i=0; i<=140; i++) {
		System.out.println(3*i);
	}
	
	
	for(int i=0; i<=65; i++) {
		System.out.println(i+"*"+i+"*"+"*"+i+"="+(i*i*i));
		
	}
	
	
	for(int i=0; i<=45; i++) {
		System.out.println(1+i*2+2*i*5);
	}
	
	
	for(int i=0; i<=65; i++) {
	System.out.println(1+i); 
	}
	
	
	for(int i=0; i<=10; i++) {
		System.out.println(2+i);
	}
	
	
	for(int i=0; i<=30; i++) {
		System.out.println(i+"*"+i+"*"+i+"*"+i+"="+(i*i*i*i));
	}
	
	
	for(int i=0; i<=1000; i++) {
		System.out.println(1+i);
	}
	
	
	for(int i=0; i<=19; i++) {
		System.out.println(1*i);
	}
	
	
	for(int i=0; i<=509; i++) {
		System.out.println(1*i*i+7*4*7*i);
		
	}
	
	for(int i=0; i<=69; i++) {
		System.out.println('y');
	}
	
	
	for(char i=0; i<=98; i++);{
	System.out.println('i');
	}
	
	
     
   
    

	}}

