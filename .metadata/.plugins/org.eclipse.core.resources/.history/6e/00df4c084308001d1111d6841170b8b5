package StaticClass;

public class VariablesA {
    
	static int i=132;//.....static
	int a=10;//...........non static
	public static void main(String[] args) {
		// TODO Auto-generated method stub
     
		System.out.println(i);
		i=133;
		System.out.println(i);
		
		VariablesA ab=new VariablesA();
		System.out.println(ab.a);
		ab. a =65;
		System.out.println(ab.a);
		
		VariablesA bb=new VariablesA();
		System.out.println(bb.a);
		bb. a=23;
		System.out.println(bb.a);
		
		VariablesA bc=new VariablesA();
		System.out.println(bc.a);
		bc. a=32;
		System.out.println(bc.a);
		
		VariablesA dc=new VariablesA();
		System.out.println(dc.a);
		dc. a=56;
		System.out.println(dc.a);
		
		VariablesA d=new VariablesA();
		System.out.println(d.a);
		d.a=432;
		System.out.println(d.a);
		
		VariablesA dd=new VariablesA();
		System.out.println(dd.a);
		dd.a=2134;
		System.out.println(dd.a);
		
		VariablesA da=new VariablesA();
		System.out.println(da.a);
		da.a=876;
		System.out.println(da.a);
		
		VariablesA dh=new VariablesA();
		System.out.println(dh.a);
		dh.a=5453;
		System.out.println(dh.a);
		
		VariablesA ds=new VariablesA();
		System.out.println(ds.a);
		ds.a=65765;
		System.out.println(ds.a);
		
		VariablesA ss=new VariablesA();
		System.out.println(ss.a);
		ss.a=4535;
		System.out.println(ss.a);
	}

}
