package mypackage1;

public class khalifa {

	public static void main(String[] args) {
		  
		for( int i=0; i<=3; i++) {
			for(int j=3; j<=3; j++) {
				System.out.print("     0");
			}
			System.out.println();
		}
		for(int i=0; i<=4; i++) {
			for(int j=0; j<=i; j++) {
				System.out.print("    0");
			}
			System.out.println();
		}
		
	}

}
