	public static void addition(int a,int b) {
		int c=a+b;
		System.out.println(c);
	}
	
	public static void admin(int a,int b,int c) {
		int d=a+b+c;
		System.out.println(d);
	}
	
	public static void add(int a, int b,int c,int d) {
		int e=a+b+c+d;
		System.out.println(e);
	}
	
	
	
	public static void addict(int a,int b,int c,int d,int e) {
		int f=a+b+c+d+e;
		System.out.println(f);
	}
	
	
	public static void subtraction(int a,int b) {
		int c=a-b;
		System.out.println(c);
	}
	
	
	
	
	public static void subtract(int a, int b, int c) {
		int d=a-b-c;
		System.out.println(d);
	}
	
	
	
	public static void sub(int a,int b,int c,int d) {
		int e=a-b-c-d;
		System.out.println(e);
	}
	
	
	
	public static void may(int a,int b) {
		int c=a*b;
		System.out.println(c);
	}
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
