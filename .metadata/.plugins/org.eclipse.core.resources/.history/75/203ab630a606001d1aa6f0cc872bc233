package project1;

public class variable {

	static int i=5;
	
	public static void m1() {
		int i=1;
		System.out.println(i);
	}
	
	
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
      
      i=10;
		m1();
		i=34;
		i=543;
	
		
		
		System.out.println(i);
		
	}
    
	
	
	
	
}
